{
    "expo": {
        "name": "UI",
        "slug": "ui",
        "privacy": "public",
        "sdkVersion": "32.0.0",
        "androidStatusBarColor": "#ccffcc",
        "androidStatusBar": {
            "barStyle": "light-content",
            "backgroundColor": "#ccffcc"
        },
        "platforms": ["ios", "android"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        }
    }
}
